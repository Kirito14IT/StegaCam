{
  "name": "stegacam-client",
  "version": "1.0.0",
  "private": true,
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:go": "expo start --go",
    "start:lan": "expo start --dev-client --host lan",
    "start:remote": "expo start --dev-client --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && gradlew.bat assembleRelease",
    "build:android:clean": "cd android && gradlew.bat clean assembleRelease",
    "build:android:dev": "cd android && gradlew.bat assembleDebug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.23",
    "expo": "^54.0.0",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "~19.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-media-library": "~18.2.0",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.0",
    "react-native-worklets": "0.5.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
